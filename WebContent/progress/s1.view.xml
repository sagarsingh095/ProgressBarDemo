<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="progress.s1" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Title">
		<content>
		<HBox >
		
			<VBox class="a">
			
			
		<Text text="Daily Analysis" class="sapUiSmallMarginBottom" />
			<ProgressIndicator
			
				class="sapUiSmallMarginBottom"
				percentValue="30"
				displayValue="30%"
				showValue="true"
				state="None" />
			<ProgressIndicator
				class="sapUiSmallMarginBottom"
				percentValue="50"
				showValue="false"
				state="Error" />
			<ProgressIndicator
				class="sapUiSmallMarginBottom"
				percentValue="99"
				displayValue="0.99GB of 1GB"
				showValue="true"
				state="Success" />
			<ProgressIndicator
				class="sapUiSmallMarginBottom"
				percentValue="25"
				displayValue="25%"
				showValue="true"
				state="Warning" />
			<ProgressIndicator
				class="sapUiSmallMarginBottom"
				percentValue="40"
				displayValue="40%"
				showValue="true"
				state="Information" />
			
			<ProgressIndicator
				class="sapUiSmallMarginBottom"
				percentValue="40"
				displayValue="yyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyy"
				showValue="true"
				state="Success" />
			
		
				</VBox>
				
				
				</HBox>
		</content>
	</Page>
</core:View>